  <!--CAROUSEL SECTION-->
  <section id="abt-carousel">

    <div id="carouselExampleFade" class="carousel slide carousel-fade bg-light" data-bs-ride="carousel"
      data-bs-interval="1000" data-bs-pause="false">
      <div class="carousel-inner img-fluid">
        <div class="carousel-item active">
          <img src="/love-dog-1.jpg" class="d-block w-100 rounded" alt="...">
        </div>
        <div class="carousel-item">
          <img src="/love-dog-2.jpg" class="d-block w-100 rounded" alt="...">
        </div>
        <div class="carousel-item">
          <img src="/love-dog-3.jpg" class="d-block w-100 rounded" alt="...">
        </div>
      </div>
      <button class="carousel-control-prev carousel-dark" type="button" data-bs-target="#carouselExampleFade"
        data-bs-slide="prev">
        <span class="carousel-control-prev-icon "></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next carousel-dark" type="button" data-bs-target="#carouselExampleFade"
        data-bs-slide="next">
        <span class="carousel-control-next-icon"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
  </section>
</main>


